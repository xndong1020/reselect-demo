{
  "name": "react-redux-boilerplate",
  "description": "React boilerplate with Redux",
  "homepage": "https://xndong1020.github.io/reselect-demo/",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./configs/webpack.config.prod.js",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./configs/webpack.config.dev.js --port 3000 --open",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "immer": "2.1.4",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-redux": "6.0.1",
    "react-router-dom": "5.0.0",
    "react-toastify": "4.5.2",
    "redux": "^4.0.1",
    "reselect": "4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.3",
    "autoprefixer": "^9.5.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-preset-react-app": "7.0.2",
    "clean-webpack-plugin": "^2.0.1",
    "compression-webpack-plugin": "^2.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "cssnano": "4.1.10",
    "enzyme": "3.9.0",
    "enzyme-adapter-react-16": "1.11.2",
    "eslint": "5.16.0",
    "eslint-loader": "2.1.2",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-react": "7.12.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fetch-mock": "7.3.1",
    "gh-pages": "^2.0.1",
    "html-webpack-plugin": "3.2.0",
    "http-server": "0.11.1",
    "jest": "24.6.0",
    "json-server": "0.14.2",
    "mini-css-extract-plugin": "0.5.0",
    "node-fetch": "^2.3.0",
    "node-sass": "^4.11.0",
    "npm-run-all": "4.1.5",
    "offline-plugin": "^5.0.6",
    "postcss-loader": "3.0.0",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react-dev-utils": "^8.0.0",
    "react-test-renderer": "16.8.6",
    "react-testing-library": "6.0.4",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.5.3",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.3.0",
    "webpack-dev-server": "3.2.1",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=8"
  },
  "babel": {
    "presets": [
      "babel-preset-react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "no-debugger": "off",
      "no-console": "off",
      "no-unused-vars": "warn",
      "react/prop-types": "warn"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "root": true
  }
}
